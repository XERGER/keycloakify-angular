{
  "name": "keycloakify-starter-angular",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "postbuild": "rimraf build/keycloak-resources",
    "test": "ng test",
    "serve": "ng serve",
    "build-keycloak-theme": "ng build && keycloakify build",
    "_format": "prettier '**/*.{ts,json,html}'",
    "format": "npm run _format --write",
    "lint": "ng lint"
  },
  "keycloakify": {
    "projectBuildDirPath": "build",
    "staticDirPathInProjectBuildDirPath": "static",
    "publicDirPath": "src/keycloak-theme/assets",
    "accountThemeImplementation": "none"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/platform-browser": "^18.2.0",
    "@angular/router": "^18.2.0",
    "keycloakify": "10.0.0-rc.131",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^18.0.0",
    "@angular-devkit/build-angular": "^18.2.0",
    "@angular/cli": "^18.2.0",
    "@angular/compiler-cli": "^18.2.0",
    "@types/node": "^15.3.1",
    "codelyzer": "^6.0.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.7",
    "tslint": "~6.1.0",
    "typescript": "~5.4.2"
  }
}
